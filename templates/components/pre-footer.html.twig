			{% block feed %}
				{% include '@W3CWebsiteTemplates/components/styles/feed.html.twig' with { 'feed_url': 'https://www.w3.org/blog/feed', 'feed_type': 'blog' } %}
			{% endblock %}

			{% block content %}
			<div>
				<h2 class="txt-jupiter">Keep updated</h2>
