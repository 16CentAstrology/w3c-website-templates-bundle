{% extends '@W3CWebsiteTemplates/bundles/TwigBundle/Exception/error.html.twig' %}

{% block documenttitle %}{{ 'error.410.title'|trans({}, 'w3c_website_templates_bundle') }} | W3C{% endblock documenttitle %}

{% block title %}{{ 'error.410.title'|trans({}, 'w3c_website_templates_bundle') }}{% endblock title %}

{% block intro %}
    {% embed '@W3CWebsiteTemplates/components/text.html.twig' %}
        {% block text %}
            {{ 'error.intro'|trans({'status_code': status_code, 'status_text': status_text}, 'w3c_website_templates_bundle')|raw }}
        {% endblock %}
    {% endembed %}
{% endblock %}

{% block content %}
    {% embed '@W3CWebsiteTemplates/components/text.html.twig' %}
        {% block text %}
            {{ 'error.410.message'|trans({}, 'w3c_website_templates_bundle')|raw }}
        {% endblock %}
    {% endembed %}
{% endblock content %}

{% block contact %}
    {% embed '@W3CWebsiteTemplates/components/text.html.twig' %}
        {% block text %}
            {{ 'error.410.contact'|trans({}, 'w3c_website_templates_bundle')|raw }}
        {% endblock %}
    {% endembed %}
{% endblock %}
